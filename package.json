  "devDependencies": {
    "@angular-builders/custom-webpack": "17.0.0",
    "@angular-builders/jest": "17.0.0",
    "@angular-devkit/build-angular": "17.0.6",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular/cli": "17.0.6",
    "@angular/compiler-cli": "17.0.6",
