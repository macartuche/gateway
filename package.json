    "ngx-mat-timepicker": "^17.0.2",
    "ngx-scrollbar": "^12.0.0",
    "ngx-spinner": "^16.0.2",
    "ngx-toastr": "^17.0.2",
    "pdfmake": "^0.2.7",
    "rxjs": "7.8.1",
    "sweetalert2": "^11.7.28",
    "tslib": "2.6.2",
    "zone.js": "0.14.2"
